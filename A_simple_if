- name: simple if loop
  hosts: all
  vars:
   names: 
      - name1.play
      - name2.play
      - name3.play 
  vars_files:
  - variables.yaml    
  tasks:
  - name: tassk1
    ansible.builtin.debug:
        msg: "{{ item }}"
    vars:
      names:
      - name1.task
      - name2.task
      - name3.task
    loop: "{{ names }}"

  - name: tassk1
    ansible.builtin.debug:
        msg: "{{ item }}"
    loop: "{{ names }}"  


